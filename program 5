#include <iostream>
using namespace std;
int main() {
    float x, y;
    cout << "Enter two floating-point numbers: ";
    cin >> x >> y;
    cout << "Addition: " << x + y << endl;
    cout << "Subtraction: " << x - y << endl;
    cout << "Multiplication: " << x * y << endl;
    cout << "Division: " << x / y << endl;
    return 0;
}
